{
    "manifest_version": 2,
    "name": "React Template",
    "version": "1.0.3",
    "description": "",
    "default_locale": "en",
    "icons": {
        "128": "./assets/images/sticky_notes_124x124.jpg"
    },
    "background": {
        "scripts": ["./background/background.js"]
    },
    "browser_action": {
        "default_title": "Sticky Note",
        "default_icon": "./assets/images/icon_124x124.jpg",
        "default_popup": "./popup/popup.html"
    },
    "options-page": {
        "page": "./options-page/index.html",
        "open_in_tab": false
    },
    "content_scripts": [
        {
            "run_at": "document_end",
            "matches": ["<all_urls>"],
            "js": ["./content_script/content_script.js"]
        }
    ],
    "permissions": [],
    "web_accessible_resources": []
}
